interface gei_1/4/1
  no shutdown
  hybrid-attribute fiber
  negotiation auto
  flowcontrol disable
  linktrap enable
  switchport mode hybrid
  switchport vlan 100,200,500 tag
  port-protect disable
  uplink-isolate disable
!
interface xgei_1/4/2
